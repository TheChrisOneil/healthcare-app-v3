{
  "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "lib": ["es2018"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "shared-interfaces/*": ["../shared-interfaces/src/*"]
    },
    "outDir": "./dist",
    "rootDir": "../"
  },
  "include": ["src/**/*", "../shared-interfaces/src/**/*", "src/swagger-config.ts"],
  "exclude": ["node_modules", "dist"]
}
